<?xml version="1.0" encoding="utf-8"?>
<shape
		xmlns:android="http://schemas.android.com/apk/res/android">
		<!-- 内部颜色 -->
		<solid
				android:color="#00000000" />
		<!-- 边缘线条颜色 -->
		<stroke
				android:width="1dp"
				android:color="#ff495a" />
		<!-- 圆角的幅度 -->
