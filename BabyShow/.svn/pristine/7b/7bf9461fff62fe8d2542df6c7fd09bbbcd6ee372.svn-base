<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    tools:context="${relativePackage}.${activityClass}" >

    <RelativeLayout
        android:id="@+id/item_hont_click_01"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="5dp"
        android:background="#ffffff" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="3dp"
            android:layout_height="15dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:background="@drawable/toys_all_category_hint_line" />

        <TextView
            android:id="@+id/item_hont_tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/imageView1"
            android:text="TextView"
            android:textColor="#333333"
            android:textSize="14dp" />

        <TextView
            android:id="@+id/item_hont_tv_title_more"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:gravity="center"
            android:text="TextView"
            android:textColor="#999999"
            android:textSize="12dp" />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dp"
            android:paddingTop="1dp"
            android:src="@drawable/toys_main_list_right" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#f5f5f5" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:background="#ffffff"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/item_hont_click_02"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:padding="5dp" >

                <TextView
                    android:id="@+id/item_hont_tv_01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:ellipsize="end"
                    android:maxLines="2"
                    android:text="tViewTextextView"
                    android:textColor="#333333"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/item_hont_pr_01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/item_hont_tv_01"
                    android:layout_marginTop="5dp"
                    android:text="TextView"
                    android:textColor="#fd6363"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/item_hont_pr_01_unit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/item_hont_pr_01"
                    android:layout_toRightOf="@+id/item_hont_pr_01"
                    android:text="TextView"
                    android:textColor="#fd6363"
                    android:textSize="12dp" />

                <ImageView
                    android:id="@+id/item_hont_iv_01"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/item_hont_pr_01"
                    android:layout_below="@+id/item_hont_pr_01"
                    android:layout_marginTop="20dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_launcher" />
            </RelativeLayout>
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:background="#f5f5f5" />

        <LinearLayout
            android:id="@+id/item_hont_all_right"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="#ffffff"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/item_hont_click_03"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:padding="5dp" >

                <TextView
                    android:id="@+id/item_hont_tv_02"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_toLeftOf="@+id/item_hont_iv_02"
                    android:ellipsize="end"
                    android:maxLines="2"
                    android:text="tViewTextextView"
                    android:textColor="#333333"
                    android:textSize="12dp" />

                <ImageView
                    android:id="@+id/item_hont_iv_02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_launcher" />

                <ImageView
                    android:id="@+id/item_hont_ivv_02"
                    android:layout_width="50dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/item_hont_tv_02"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/toys_all_todo" />

                <TextView
                    android:id="@+id/item_hont_pr_02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/item_hont_ivv_02"
                    android:layout_marginTop="5dp"
                    android:text="TextView"
                    android:textColor="#fd6363"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/item_hont_pr_02_unit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/item_hont_pr_02"
                    android:layout_toRightOf="@+id/item_hont_pr_02"
                    android:text="TextView"
                    android:textColor="#fd6363"
                    android:textSize="10dp" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#f5f5f5" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="#ffffff"
                android:orientation="horizontal" >

                <RelativeLayout
                    android:id="@+id/item_hont_click_04"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <TextView
                        android:id="@+id/item_hont_tv_03"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:ellipsize="end"
                        android:maxLines="1"
                        android:text="Textext"
                        android:textColor="#333333"
                        android:textSize="12dp" />

                    <RelativeLayout
                        android:id="@+id/item_hont_pr_03_all"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/item_hont_tv_03"
                        android:layout_marginTop="3dp"
                        android:gravity="center" >

                        <TextView
                            android:id="@+id/item_hont_pr_03"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="TextView"
                            android:textColor="#fd6363"
                            android:textSize="12dp" />

                        <TextView
                            android:id="@+id/item_hont_pr_03_unit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/item_hont_pr_03"
                            android:layout_toRightOf="@+id/item_hont_pr_03"
                            android:text="Te"
                            android:textColor="#fd6363"
                            android:textSize="10dp" />
                    </RelativeLayout>

                    <ImageView
                        android:id="@+id/item_hont_iv_03"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/item_hont_pr_03_all"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="3dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/ic_launcher" />
                </RelativeLayout>

                <View
                    android:layout_width="1dp"
                    android:layout_height="fill_parent"
                    android:background="#f5f5f5" />

                <RelativeLayout
                    android:id="@+id/item_hont_click_05"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:padding="5dp" >

                    <TextView
                        android:id="@+id/item_hont_tv_04"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:ellipsize="end"
                        android:maxLines="1"
                        android:text="Textext"
                        android:textColor="#333333"
                        android:textSize="12dp" />

                    <RelativeLayout
                        android:id="@+id/item_hont_pr_04_all"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/item_hont_tv_04"
                        android:layout_below="@+id/item_hont_tv_04"
                        android:layout_marginTop="3dp"
                        android:gravity="center" >

                        <TextView
                            android:id="@+id/item_hont_pr_04"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="TextView"
                            android:textColor="#fd6363"
                            android:textSize="12dp" />

                        <TextView
                            android:id="@+id/item_hont_pr_04_unit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/item_hont_pr_04"
                            android:layout_toRightOf="@+id/item_hont_pr_04"
                            android:text="Te"
                            android:textColor="#fd6363"
                            android:textSize="10dp" />
                    </RelativeLayout>

                    <ImageView
                        android:id="@+id/item_hont_iv_04"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/item_hont_pr_04_all"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="3dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/ic_launcher" />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>