	public ToysDetailBean fromJson(Context context, JSONObject json) {
		ToysDetailBean toysDetailBean = new ToysDetailBean();
		try {
			ToysDetial detai = new ToysDetial();
			detai.setBusiness_id(json.getJSONObject("detail").getString("business_id"));
			detai.setIs_order(json.getJSONObject("detail").getString("is_order"));
			detai.setIs_support(json.getJSONObject("detail").getString("is_support"));
			detai.setImg_thumb(json.getJSONObject("detail").getString("img_thumb"));
			detai.setOrder_status(json.getJSONObject("detail").getString("order_status"));
			detai.setIs_cart(json.getJSONObject("detail").getString("is_cart"));
			detai.setIs_cart_number(json.getJSONObject("detail").getString("is_cart_number"));
			detai.setIs_change(json.getJSONObject("detail").getString("is_change"));
			detai.setCart_number(json.getJSONObject("detail").getString("cart_number"));
			detai.setIs_appoint(json.getJSONObject("detail").getString("is_appoint"));
			
			ShoppingCartUtils.setCartNumber(context, Integer.parseInt(json.getJSONObject("detail").getString("cart_number")));
			
			ToysDescription description = new ToysDescription();
			
			ToysPrice price = new ToysPrice();
			price.setNew_price_button(json.getJSONObject("price").getString("new_price_button"));
			price.setPrice_button(json.getJSONObject("price").getString("price_button"));
			
			toysDetailBean.setDetail(detai);
			toysDetailBean.setDescription(description);
			toysDetailBean.setPrice(price);
			toysDetailBean.setToys_des(json.getString("toys_description"));
			
		} catch (JSONException e) {
			e.printStackTrace();
		}
		return toysDetailBean;
	}
	
